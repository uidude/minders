{
  "expo": {
    "name": "Minders",
    "slug": "minders",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../client/assets/icon.png",
    "splash": {
      "image": "../client/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uidude.minders",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.316194284018-4iucu1np3ngg7n3ngprre8iro04oe173"
        },
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../client/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.uidude.minders"
    },
    "web": {
      "favicon": "../client/assets/favicon.png"
    },
    "facebookScheme": "fb",
    "facebookAppId": "",
    "facebookDisplayName": "minders",
    "extra": {
      "eas": {
        "projectId": "2f07f39a-d4fd-449f-ab0e-96b438be599d"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "13.0"
          }
        }
      ]
    ]
  }
}
